<div class="animated fadeIn">
    <div class="card" style="border-radius: 10px;">
        <div class="card-content" style="padding:0 20px 0 20px;">
            <div class=" row">
                <div class="col-md-7">
                    <h2 style="font-weight:bold;">Dashboard</h2>
                    <ol class="breadcrumb">
                        <li>
                            Dashboard
                        </li>
                        <li class="active">
                        </li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="card" style="border-radius:10px; padding: 15px;">

    <div class="row" style="margin: 10px 0 10px 0;">
        <div class="col-md-5" style="background-color: #EEEEEE; height:2px; margin-top:18px;">

        </div>
        <div class="col-md-2" style="font-weight:bold; font-size:25px; text-align:center;">
            {{myprojects}}
        </div>
        <div class="col-md-5" style="background-color: #EEEEEE; height:2px; margin-top:18px;">

        </div>
    </div>

    <div class="animated fadeIn">
        <div class="row">
            <div class="col-md-4">
                <div class="mini-card" style="background-color:#018D4D">
                    <div class="mini-icon">
                        <i class="glyphicon glyphicon-ok-sign"></i>
                        <div class="mini-count">
                            <div class="label">
                                Completed: {{statCount.Completed}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="mini-card" style="">
                    <div class="mini-icon">
                        <i class="fa fa-circle-o"></i>
                        <div class="mini-count">
                            <div class="label">
                                Active: {{statCount.Active}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="mini-card" style="background-color:#E7E7E7">
                    <div class="mini-icon">
                        <i class="fa fa-close" style="color:black;"></i>
                        <div class="mini-count">
                            <div class="label" style="color:black;">
                                Cancelled: {{statCount.Cancelled}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="mytasks" class="row" style="margin: 10px 0 10px 0; display:none;">
        <div class="col-md-5" style="background-color: #EEEEEE; height:2px; margin-top:18px;">

        </div>
        <div class="col-md-2" style="font-weight:bold; font-size:25px; text-align:center;">
            My Tasks
        </div>
        <div class="col-md-5" style="background-color: #EEEEEE; height:2px; margin-top:18px;">

        </div>
    </div>

    <div id="mytaskcontent" class="animated fadeIn" style="display:none;">
        <div class="row">
            <div class="col-md-4">
                <div class="mini-card" style="background-color:#018D4D">
                    <div class="mini-icon">
                        <i class="glyphicon glyphicon-ok-sign"></i>
                        <div class="mini-count">
                            <div class="label">
                                Completed: {{taskCount.Completed}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="mini-card" style="background-color:#F39C15">
                    <div class="mini-icon">
                        <i class="fa fa-clock-o"></i>
                        <div class="mini-count">
                            <div class="label">
                                Pending: {{taskCount.Pending}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="mini-card">
                    <div class="mini-icon">
                        <i class="fa fa-circle-o" style="color:white;"></i>
                        <div class="mini-count">
                            <div class="label" style="color:white;">
                                Available: {{taskCount.Available}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="myactivites" class="row" style="margin: 10px 0 10px 0; display:none;" >
        <div class="col-md-5" style="background-color: #EEEEEE; height:2px; margin-top:18px;">

        </div>
        <div class="col-md-2" style="font-weight:bold; font-size:25px; text-align:center;">
            My Activities
        </div>
        <div class="col-md-5" style="background-color: #EEEEEE; height:2px; margin-top:18px;">

        </div>
    </div>

    <div id="myactivitescont" class="animated fadeIn" style="display:none;">
        <ul id="ultimeline" class="timeline">
            <!-- timeline time label -->
            <li class="" ng-repeat="activity in activities" ng-init="timelabel(activity.date)" ng-click="changeURL(activity.id)" style="cursor:pointer;">
                <i ng-if="true" class="{{timelineIcon(activity.type)}}" style="color:white;"></i>
                <div ng-if="true" class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> {{convertJsonDate(activity.date)}}</span>
                    <h3 class="timeline-header no-border"><a href="#"></a> {{activity.actcontent}}</h3>
                </div>
            </li>
            <li class="time-label">
                <span class="bg-red" style="color:white;">
                    10 Feb. 2014
                </span>
            </li>
            <!-- /.timeline-label -->
            <!-- timeline item -->
            <li>
                <i class="fa fa-tasks bg-blue" style="color:white;"></i>
                <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> 12:05</span>
                    <h3 class="timeline-header"><a href="#">Support Team</a> sent you an email</h3>
                    <div class="timeline-body">
                        Etsy doostang zoodles disqus groupon greplin oooj voxy zoodles,
                        weebly ning heekya handango imeem plugg dopplr jibjab, movity
                        jajah plickers sifteo edmodo ifttt zimbra. Babblely odeo kaboodle
                        quora plaxo ideeli hulu weebly balihoo...
                    </div>
                    <div class="timeline-footer">
                        <a class="btn btn-primary btn-xs">Read more</a>
                        <a class="btn btn-danger btn-xs">Delete</a>
                    </div>
                </div>
            </li>
            <!-- END timeline item -->
            <!-- timeline item -->
            <li>
                <i class="fa fa-user bg-aqua" style="color:white;"></i>
                <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> 5 mins ago</span>
                    <h3 class="timeline-header no-border"><a href="#">Sarah Young</a> accepted your friend request</h3>
                </div>
            </li>
            <!-- END timeline item -->
            <!-- timeline item -->
            <li>
                <i class="fa fa-comments bg-yellow" style="color:white;"></i>
                <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> 27 mins ago</span>
                    <h3 class="timeline-header"><a href="#">Jay White</a> commented on your post</h3>
                    <div class="timeline-body">
                        Take me to your leader!
                        Switzerland is small and neutral!
                        We are more like Germany, ambitious and misunderstood!
                    </div>
                    <div class="timeline-footer">
                        <a class="btn btn-warning btn-flat btn-xs">View comment</a>
                    </div>
                </div>
            </li>
            <!-- END timeline item -->
            <!-- timeline time label -->
            <li class="time-label">
                <span class="bg-green" style="color:white;">
                    3 Jan. 2014
                </span>
            </li>
            <!-- /.timeline-label -->
            <!-- timeline item -->
            <li>
                <i class="fa fa-camera bg-purple" style="color:white;"></i>
                <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> 2 days ago</span>
                    <h3 class="timeline-header"><a href="#">Mina Lee</a> uploaded new photos</h3>
                    <div class="timeline-body">
                        <img src="http://placehold.it/150x100" alt="..." class="margin">
                        <img src="http://placehold.it/150x100" alt="..." class="margin">
                        <img src="http://placehold.it/150x100" alt="..." class="margin">
                        <img src="http://placehold.it/150x100" alt="..." class="margin">
                    </div>
                </div>
            </li>
            <!-- END timeline item -->
            <!-- timeline item -->
            <li>
                <i class="fa fa-video-camera bg-maroon" style="color:white;"></i>
                <div class="timeline-item">
                    <span class="time" style="color:white;"><i class="fa fa-clock-o"></i> 5 days ago</span>
                    <h3 class="timeline-header"><a href="#">Mr. Doe</a> shared a video</h3>
                    <div class="timeline-body">
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe class="embed-responsive-item" src="https://www.youtube.com/embed/tMWkeBIohBs" frameborder="0" allowfullscreen=""></iframe>
                        </div>
                    </div>
                    <div class="timeline-footer">
                        <a href="#" class="btn btn-xs bg-maroon">See comments</a>
                    </div>
                </div>
            </li>
            <!-- END timeline item -->
            <li>
                <i class="fa fa-clock-o bg-gray"></i>
            </li>
        </ul>
    </div>
</div>